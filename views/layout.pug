    title= title
    meta(charset='utf-8')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(http-equiv="Content-Security-Policy" content="default-src 'self' 'unsafe-hashes' 'unsafe-inline'; style-src 'unsafe-inline' 'self'; script-src 'unsafe-inline' 'self' 'https://unpkg.com/@babel/standalone/babel.min.js';")
    meta(http-equiv="X-Content-Security-Policy" content="default-src 'self' 'unsafe-hashes' 'unsafe-inline'; style-src 'unsafe-inline' 'self'; script-src 'unsafe-inline' 'self' 'https://unpkg.com/@babel/standalone/babel.min.js';")
    link(rel='stylesheet', href='/stylesheets/stylesheet.css')    
    
  body
    div(class='container-fluid')
      h1 Title
      div(class='row')
        div(class='col-sm-2')
          block sidebar
            ul(class='sidebar-nav menu')
              li
                a(href='/users') users
              li
                a(href='/index.php') index
              li(id='accountLink')
        div(class='col-sm-10 main')
          block content
        script(src="/javascripts/storage.js")
